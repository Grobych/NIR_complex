<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" minHeight="600.0" minWidth="1000.0" prefHeight="900.0" prefWidth="1500.0" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nir.controller.Controller">
   <!--<columnConstraints>-->
      <!--<ColumnConstraints minWidth="10.0" prefWidth="800.0" />-->
