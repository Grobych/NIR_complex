      <TableView prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1">
        <columns>
          <TableColumn prefWidth="99.0" text="param" />
          <TableColumn prefWidth="100.0" text="value" />
        </columns>
      </TableView>
