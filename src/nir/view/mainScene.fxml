   <children>
      <Pane prefHeight="700.0" prefWidth="1200.0">
         <children>
            <Canvas fx:id="mapCanvas" height="700.0" layoutX="10.0" layoutY="13.0" width="1200.0"/>
            <Canvas fx:id="robotCanvas" height="700.0" layoutX="10.0" layoutY="13.0" width="1200.0"/>
            <Canvas fx:id="utilCanvas" height="700.0" layoutX="10.0" layoutY="13.0" width="1200.0"/>
            <Canvas fx:id="pheromoneCanvas" height="700.0" layoutX="10.0" layoutY="13.0" width="1200.0"/>
         </children>
      </Pane>
      <VBox prefHeight="200.0" prefWidth="119.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
