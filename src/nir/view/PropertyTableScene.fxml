<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="nir.controller.PropertyTableController"
            prefHeight="400.0" prefWidth="600.0">

    <TableView fx:id="paramTableView" editable="true">
        <columns>
            <TableColumn fx:id="paramColumn" prefWidth="99.0" text="param" editable="true" />
            <TableColumn fx:id="valueColumn" prefWidth="100.0" text="value" editable="true" />
        </columns>
    </TableView>
</AnchorPane>
